<span><%= t(:seller) %>:</span>
<% if seller_signed_in? %>
  <%= link_to t(:log_out), destroy_seller_session_path, method: :delete %>
  <%= link_to t(:account), seller_dashboard_path(current_seller) %>
<% else %>
  <%= link_to t(:sign_in), new_seller_session_path %>
<% end%>

<% if admin_user_signed_in? %>
  <%= link_to t(:log_out), destroy_admin_user_session_path, method: :delete %>
<% end%>
